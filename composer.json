{
    "name": "unisharp/s3-presigned",
    "description": "An AWS S3 package for pre-signed upload purpose in Laravel and PHP.",
    "keywords": ["aws", "s3", "pre-signed", "laravel", "php", "upload"],
    "license": "MIT",
    "authors": [
        {
            "name": "UniSharp Ltd.",
            "email": "unisharp-service@unisharp.com"
        }
    ],
    "require": {
        "php": "^5.6.4 || ^7.0",
        "aws/aws-sdk-php": "^3.31",
        "illuminate/support": ">=5.0.0",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1",
        "mockery/mockery": "^1.2.4"
    },
    "autoload": {
        "psr-4": {
            "Unisharp\\S3\\Presigned\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Unisharp\\S3\\Presigned\\S3PresignedServiceProvider"
            ],
            "aliases": {
            }
        }
    }
}
